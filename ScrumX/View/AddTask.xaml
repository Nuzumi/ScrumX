<Window x:Class="ScrumX.View.AddTask"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScrumX.View"
        mc:Ignorable="d"
        Title="AddTask" Height="300" Width="300" Name="Window" WindowStyle="None">
    <Grid>
        <Button Content="ok" Command="{Binding AddTaskCommand}" CommandParameter="{Binding ElementName=Window}" />
    </Grid>
</Window>
